{
    "Comment": "An example Step Functions state machine that executes a Lambda function with a variable",
    "StartAt": "getVideoId",
    "States": {
      "getVideoId": {
        "Type": "Task",
        "Resource": "${aws_lambda_function.default.arn}",
        "Parameters": {
          "input": {
            "action_name": "get_video_id",
            "videoName.$": "$.videoName",
          }
        },
        "ResultPath": "$.lambdaOutput",
        "Next": "Success"
      },
      "Success": {
        "Type": "Succeed"
      }
    }
  }
  